{
    "appName": "angular-nest-pg-library",
    "packageType": "npm",
    "skipPackageInstallation": true,
    "children": [{
        "routePrefix": "books",
        "routes": [{
            "requestType": "GET",
            "routeName": "",
            "functionName": "books"
        }, {
            "requestType": "GET",
            "routeName": "searchBooks",
            "functionName": "searchBooks",
            "body": ["bookName", "author", "minPages", "maxPages"]
        }, {
            "requestType": "GET",
            "routeName": "getBook",
            "functionName": "getBook",
            "body": ["id"]
        }, {
            "requestType": "POST",
            "routeName": "",
            "functionName": "saveBook",
            "body": ["bookName", "author", "pageCount"]
        },{
            "requestType": "PUT",
            "routeName": "",
            "functionName": "updateBook",
            "body": ["bookName", "author", "pageCount"]
        }, {
            "requestType": "DELETE",
            "routeName": "",
            "functionName": "deleteBook",
            "body": ["id"]
        }]
    }]
}